--effects
function explode(x,y,r,c_table,num)
	for i=0, num do
	--settings
	add_fx(
		x,         -- x
		y,         -- y
		30+rnd(25),-- die
		rnd(2)-1,  -- dx
		rnd(2)-1,  -- dy
		false,     -- gravity
		false,     -- grow
		true,      -- shrink
		r,         -- radius
		c_table    -- color_table
		)
	end
end

function add_fx(x,y,die,dx,dy,grav,grow,shrink,r,c_table)
	local fx={
		x=x,
		y=y,
	t=0,
	die=die,
	dx=dx,
	dy=dy,
	grav=grav,
	grow=grow,
	shrink=shrink,
	r=r,
	c=0,
	c_table=c_table
	}
	add(effects,fx)
end


