function init_settings()
	settings=1 //standard markeret
	--standard map
	arrow1=35
	--standard gun
	arrow2=75 
end
       
function update_settings()
	if btn(➡️) then scene=2 end
	if btnp(⬇️) then settings+=1 end
	if btnp(⬆️) then settings-=1 end
	if settings>6 then settings=1 end
	if settings<1 then settings=6 end
	config_map={
		[1]=function() arrow1=35 maps.x=0 
			maps.nr=1 maps.col=11 maps.sp=199 end,
		[2]=function() arrow1=43 maps.x=16 
			maps.nr=2 maps.col=3 maps.sp=168 end,
		[3]=function() arrow1=51 maps.x=32 
			maps.nr=3 maps.col=7 maps.sp=235 end} 
	if btn(❎) and settings>0 and settings<4
		then config_map[settings]() end
	config_gun={
		[4]=function() arrow2=67 gun.sp=76 
			gun.s=23 gun.nr=1 gun.b=16 end,
		[5]=function() arrow2=75 gun.sp=156 
			gun.s=21 gun.nr=2 gun.b=17 end,
		[6]=function() arrow2=83 gun.sp=72 
			gun.s=22 gun.nr=3 gun.b=18 end}
	if btn(❎) and settings>3 and settings<7 
		then config_gun[settings]() end
end

