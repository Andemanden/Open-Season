--animation
function anim(sp,w,mx,mn,an_t,an_w)
	//sp=sprite w=tykkelse
	//mx=max sp  mn=min sp
	//an_t=tids var an_w=wait var
	if time()-an_t>an_w then
		sp+=w
		an_t=time()
		if sp>mx then
			sp=mn
		end
	end
	
	return sp
end
