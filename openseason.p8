--scroller, menu og settings--
       --scroller--
function update_scroller()
	if time()>1 then 
		scene=2 
		init_settings() end

end


function draw_scroller()
cls()
	print("scroller") //test screen
end

        --menu--
function update_menu()
	if btn(❎) then scene=3 end
	if btn(⬅️) then scene=6 end
end


function draw_menu()
	pal(10,12+128,1) //aendre baggrund
	cls(10)
	pal(14,6+128,1)
	pal(6,0+128,1)
	pal(5,3+128,1)
	color(7)
	spr(192,36,8,7,2)
	spr(224,20,26,11,2)
	print("⬅️ for settings",30,100,7)
	print("press ❎ to start",28,108,8) 
end
       --settings--
function init_settings()
	settings=1 //standard markeret
	--standard map
	arrow1=35
	maps.x=0
	--standard gun
	arrow2=75 
	gun.sp=33
end
       
function update_settings()
	if btn(➡️) then scene=2 end
	if btnp(⬇️) then settings+=1 end
	if btnp(⬆️) then settings-=1 end
	if settings>6 then settings=1 end
	if settings<1 then settings=6 end
	config_map={
		[1]=function() arrow1=35 maps.x=0 end,
		[2]=function() arrow1=43 maps.x=128 end,
		[3]=function() arrow1=51 maps.x=256 end} 
	if btn(❎) and settings>0 and settings<4
		then config_map[settings]() end
	config_gun={
		[4]=function() arrow2=67 gun.sp=32 end,
		[5]=function() arrow2=75 gun.sp=33 end,
		[6]=function() arrow2=83 gun.sp=34 end}
	if btn(❎) and settings>3 and settings<7 
		then config_gun[settings]() end
end

function draw_settings()
	cls(10)
	pal(10,12+128,1)
	pal(13,2+128,1)
	print("settings",46,17,7)
	print("settings",46,16,8)
	color(7)
	print("map",56,28)
	line(16,34,116,34)
	spr(02,14,arrow1)
	spr(02,14,arrow2)
	print("safari",20,36,7)
	print("jungle",20,44,7)
	print("polar",20,52,7)
	print("weapon",48,60)
	line(16,66,116,66)
	print("double barrel shotgun",20,68,7)
	print("single barrel shotgun",20,76,7)
	print("rifle",20,84,7)
	
	draw_map={
		[1]=function() print("safari",20,36,8) end,
		[2]=function() print("jungle",20,44,8) end,
		[3]=function() print("polar",20,52,8) end}
	if settings>0 and settings<4 
		then draw_map[settings]() end
	draw_gun={
		[4]=function() print("double barrel shotgun",20,68,8) end,
		[5]=function() print("single barrel shotgun",20,76,8) end,
		[6]=function() print("rifle",20,84,8) end} 
	if settings>3 and settings<7 then
	 draw_gun[settings]() end 
	print("➡️ for menu",40,114,7)
	print("press ❎ to choose",28,120,8)
	
	color(7)
end

