function init_settings()
	settings=1 //standard markeret
	--standard map
	arrow1=35
	maps.x=0
	--standard gun
	arrow2=75 
	gun.sp=33
end
       
function update_settings()
	if btn(âž¡ï¸) then scene=2 end
	if btnp(â¬‡ï¸) then settings+=1 end
	if btnp(â¬†ï¸) then settings-=1 end
	if settings>6 then settings=1 end
	if settings<1 then settings=6 end
	config_map={
		[1]=function() arrow1=35 maps.x=0 maps.nr=1 end,
		[2]=function() arrow1=43 maps.x=16 maps.nr=2 end,
		[3]=function() arrow1=51 maps.x=32 maps.nr=3 end} 
	if btn(âŽ) and settings>0 and settings<4
		then config_map[settings]() end
	config_gun={
		[4]=function() arrow2=67 gun.sp=32 gun.s=23 end,
		[5]=function() arrow2=75 gun.sp=33 gun.s=21 end,
		[6]=function() arrow2=83 gun.sp=34 gun.s=22 end}
	if btn(âŽ) and settings>3 and settings<7 
		then config_gun[settings]() end
end

