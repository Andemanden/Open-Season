--game--
function init_game()
	clearpal()
	init_specific={
		[1]=function() end,
		[2]=function()  end,
		[3]=function() end}
	init_specific[maps.nr]()
	birds={} //alle fugle
	land_animals={} //alle land dyr
end


function add_animal(types,tbl,sprite,lives)
	//types=type of animal
	//tbl=table sp=sprite_nr
	
	if types=="land" then
		add(tbl,{sp=sprite,x=rnd(120)
		,y=102-rnd(38),dx=1-rnd(2)
		,dy=1-rnd(2),w=2,h=2
		,lvs=lives,shot=false
		,flp=false,sitting=false}) 
	elseif types=="bird" then
		add(tbl,{sp=sprite,x=rnd(120)
			,y=rnd(128),dx=rnd(2)
			,dy=rnd(2),w=1,h=1,lvs=lives
			,shot=false,flp=false})
	end
	
end

function move_animal() 
	for la in all(land_animals) do
		if la.x<112 and la.x>0 then
			la.x+=la.dx
		else
			la.dx*=-1
		end
	end
	
end

--update--
function update_game()
	move_animal() 
	
	if maps.nr==1 or maps.nr==3 then
		if #land_animals<4 then
			add_animal("land"
			,land_animals,199,3,false)
			
		end
	end
	
	if maps.nr==2 then
		if #birds<4 then
			add_animal("bird"
			,birds,238,3,false)
		end
		if #land_animals<2 then
		add_animal("land"
			,land_animals,136,3,false)
		end
	end
end




--draw
function draw_game()
	cls()
	map(maps.x,maps.y,maps.sx,maps.sy,16,16)
	draw_specific={
		[1]=function() pal(12,12+128,1) 
		end,
		[2]=function() pal(12,12+128,1)
		end,
		[3]=function()  end}
	draw_specific[maps.nr]()
	--jorden
	rectfill(0,72,128,128,maps.col)
	--bullets
	rectfill(8,112,18,126,6)
	
	pal(2,10+128,1) //busk groen
	print("game",1,1) //test screen
	--tegn dyr
	for la in all(land_animals) do
		spr(la.sp,la.x,la.y,la.w,la.h,la.flp)
	end
	for b in all(birds) do
		spr(b.sp,b.x,b.y,b.w,b.h,b.flp)
	end
	
	if stat(34)==0 then //normal
		spr(gun.s,stat(32)-1,stat(33)-1)
		spr(gun.b,10,114)
	elseif stat(34)==1 then //skyd
		spr(gun.s-16,stat(32)-1,stat(33)-1)
	else 
		//reload
	end
	
	
	
	
end


