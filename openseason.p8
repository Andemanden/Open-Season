--game--
function init_game()
	clearpal()
	init_specific={
		[1]=function() end,
		[2]=function()  end,
		[3]=function() end}
	init_specific[maps.nr]()
	birds={} //alle fugle
	land_animals={} //alle land dyr
end


function add_animal(types,tbl,sprite,w,h,lives,flp)
	//types=type of animal
	//tbl=table sp=sprite_nr
	
	if types=="land" then
		local animal={sp=sprite,x=rnd(120),y=rnd(128)
			,dx=rnd(2),dy=rnd(2),lvs=lives,shot=false,flips=flp}
	elseif types=="bird" then
		local animal={sp=sprite,x=rnd(120),y=rnd(128)
			,dx=rnd(2),dy=rnd(2),w=1,h=1,lvs=lives,shot=false,flips=flp}
	end
	add(tbl,animal)
	
end


--update--
function update_game()

	if maps.nr==1 or maps.nr==3 then
		if #land_animals<4 then
			add_animal("land",land_animals,8,1,1,3,false)
		end
	end
	
	if maps.nr==2 then
		if #birds<4 then
			add_animal("bird",birds,56,1,1,3,false)
		end
	end
end




--draw
function draw_game()
	cls()
	draw_specific={
		[1]=function() pal(12,12+128,1) end,
		[2]=function() pal(12,12+128,1) end,
		[3]=function() spr() end}
	draw_specific[maps.nr]()
	map(maps.x,maps.y,maps.sx,maps.sy,16,16)
	
	pal(2,10+128,1) //busk groen
	print("game",1,1) //test screen
	if stat(34)==0 then
		spr(gun.s,stat(32)-1,stat(33)-1)
	elseif stat(34)==1 then
		spr(gun.s-16,stat(32)-1,stat(33)-1)
	else 
		//reload
	end
	
	
	
	for la in all(land_animals) do
		spr(40,la.x,la.y,la.w,la.h,la.flp)
	end
	for b in all(birds) do
		spr(b.sp,b.x,b.y,b.w,b.h,b.flp)
	end
	
end


